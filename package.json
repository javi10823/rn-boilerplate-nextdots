{
  "name": "CoreMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "ts": "tsc --watch --noEmit --skipLibCheck",
    "prettier-format": "prettier \"src/**/*.{js,jsx,json,tsx,ts}\" --write",
    "lint": "tsc --noEmit --skipLibCheck && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --skipLibCheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run prettier-format",
      "yarn run lint",
      "prettier src --write",
      "git add"
    ],
    "*.{json}": [
      "prettier src --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@sentry/react-native": "^1.0.6",
    "react": "16.9.0",
    "react-native": "0.61.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-material-textfield": "^0.12.0",
    "react-native-reanimated": "^1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.3.2",
    "react-navigation-stack": "^1.9.3",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0",
    "tslint": "^5.20.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.18",
    "@types/react": "16.9.0",
    "@types/react-native": "^0.60.17",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-material-textfield": "^0.12.3",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "16.9.0",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-form": "^8.1.4",
    "@types/redux-logger": "^3.0.7",
    "@types/remote-redux-devtools": "^0.5.3",
    "@types/styled-components": "^4.0.3",
    "@types/yup": "^0.26.22",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "dtslint": "^0.8.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.9.0",
    "redux-logger": "^3.0.6",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
